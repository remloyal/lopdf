{
  "name": "lopdf",
  "version": "0.3.3",
  "main": "index.js",
  "types": "index.d.ts",
  "description": "Read PDF",
  "napi": {
    "name": "lopdf",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-pc-windows-msvc"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^2.18.4",
